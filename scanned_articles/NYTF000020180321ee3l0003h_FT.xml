<DistDoc action="add" dist="dist_id" distdocver="05.1" an="NYTF000020180321ee3l0003h" doctype="article"><SelectData><FolderName id="302210950" rl="100">NYT</FolderName></SelectData><ReplyItem lang="en"><BaseLang value="en" fid="la"/><IPDocId fid="id">20180321100749</IPDocId><AccessionNo value="NYTF000020180321ee3l0003h" fid="an"/><Num value="505" fid="wc"/><Date value="20180321" fid="pd"/><SrcCode value="nytf" fid="sc"/><SrcName fid="sn">The New York Times</SrcName><TruncRules><XS value="29"/><S value="60"/></TruncRules><Title fid="hl"><SectionName fid="se">Foreign Desk; SECT</SectionName><ColumnName fid="clm"></ColumnName><Headline fid="hd"><Para lang="en">Fearing New Outbreaks, Brazil Will Vaccinate Country Against Yellow Fever</Para></Headline></Title><Snippet><Para>SÃO PAULO -- Hoping to stave off another deadly outbreak of yellow fever, Brazil's government announced on Tuesday that it planned to vaccinate the entire country against the mosquito-borne virus by April 2019.</Para></Snippet><Byline fid="by">By SHASTA DARLINGTON </Byline><Credit fid="cr"></Credit><Copyright fid="cy">Copyright 2018 The New York Times Company.  All Rights Reserved. </Copyright><CodeSets><CSet fid="in"><Code value="i2572"/><Code value="i257"/><Code value="i951"/><Code value="idrugty"/></CSet><CSet fid="re"><Code value="braz"/><Code value="soap"/><Code value="bric"/><Code value="devgcoz"/><Code value="dvpcoz"/><Code value="lamz"/><Code value="samz"/></CSet><CSet fid="ns"><Code value="gout"/><Code value="gvhhs"/><Code value="gimmu"/><Code value="gmosq"/><Code value="gcat"/><Code value="ghea"/><Code value="gmed"/><Code value="gpir"/><Code value="gpol"/><Code value="gspox"/><Code value="gtrea"/><Code value="gvbod"/><Code value="gvexe"/></CSet></CodeSets></ReplyItem><MetadataPT><DocData><DocType value="article" fid="fmt"/><IPDocId fid="id">20180321100749</IPDocId><AccessionNo value="NYTF000020180321ee3l0003h" fid="an"/><Num value="505" fid="wc"/><Num value="2504" fid="cc"/><Date value="2018