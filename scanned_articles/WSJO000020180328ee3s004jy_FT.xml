<DistDoc action="add" dist="dist_id" distdocver="05.1" an="WSJO000020180328ee3s004jy" doctype="article"><SelectData><FolderName id="302210958" rl="100">WSJ</FolderName></SelectData><ReplyItem lang="en"><BaseLang value="en" fid="la"/><IPDocId fid="id">834A792B6AA816615E1413C15C45B39E SB12718948978598503280504584129850378018942</IPDocId><AccessionNo value="WSJO000020180328ee3s004jy" fid="an"/><Num value="391" fid="wc"/><Date value="20180328" fid="pd"/><Time value="202100" fid="et"/><SrcCode value="wsjo" fid="sc"/><SrcName fid="sn">The Wall Street Journal Online</SrcName><TruncRules><XS value="27"/><S value="57"/><M value="79"/><L value="139"/></TruncRules><Title fid="hl"><SectionName fid="se">US</SectionName><ColumnName fid="clm"></ColumnName><Headline fid="hd"><Para lang="en">Massachusetts Utilities Cut Ties With New Hampshire Power-Line Project; Eversource Energy's $1.6 billion Northern Pass was expected to link Massachusetts to Canadian hydroelectric dams</Para></Headline></Title><Snippet><Para>Electric utilities in Massachusetts have officially broken ties with a power-line project in New Hampshire that was envisioned to connect New England's most populous state to hydroelectric dams in Canada.</Para></Snippet><Byline fid="by">By Jon Kamp </Byline><Credit fid="cr"></Credit><Copyright fid="cy">Copyright 2018 Dow Jones &amp; Company, Inc. All Rights Reserved. </Copyright><CodeSets><CSet fid="co"><Code value="neuts"/></CSet><CSet fid="in"><Code value="i1610107"/><Code value="i1"/><Code value="i16"/><Code value="i16101"/><Code value="i163"/><Code value="iutil"/><Code value="ieutil"/><Code value="i16103"/></CSet><CSet fid="re"><Code value="usma"/><Code value="usa"/><Code value="cana"/><Code value="namz"/><Code value="use"/><Code value="usnew"/></CSet></CodeSets></ReplyItem><MetadataPT><DocData><DocType value="article" fid="fmt"/><IPDocId fid="id">834A792B6AA816615E1413C15C45B39E SB12718948978598503280504584129850378018942</IPDocId><AccessionNo value="WSJO000020180328ee